<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bwieckowski.toolbox.Controller">
   <children>
      <VBox spacing="10.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#switchScene" text="&lt;-" />
            <Button fx:id="convert" mnemonicParsing="false" onAction="#strConvert" text="PHP -&gt; SQL">
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="convert1" mnemonicParsing="false" onAction="#strConvert" text="PHP &lt;- SQL">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <VBox>
         <children>
            <Label text="PHP" />
            <TextArea fx:id="php" prefHeight="400.0" prefWidth="500.0" />
         </children>
      </VBox>
      <VBox>
         <children>
            <Label text="SQL" />
            <TextArea fx:id="sql" prefHeight="400.0" prefWidth="500.0" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
